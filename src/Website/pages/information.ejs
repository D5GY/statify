    </div>
    <div class="information">
      <div class="container mt-3">
        <div class="guild">
          <div class="section-header">
            <h1>[guilds] Information</h1>
          </div>
          <div class="items">
            <div class="d-flex justify-content-between">
              <h3>Guild Name:</h3>
              <h3>gggg</h3>
            </div>
            <hr>
            <div class="d-flex justify-content-between">
              <h3>Guild ID:</h3>
              <h3>gggg</h3>
            </div>
            <hr>
            <div class="d-flex justify-content-between">
              <h3>Guild Members:</h3>
              <h3>gggg</h3>
            </div>
            <hr>
            <div class="d-flex justify-content-between">
              <h3>Guild Bots:</h3>
              <h3>gggg</h3>
            </div>
            <hr>
            <div class="d-flex justify-content-between">
              <h3>Guild Owner:</h3>
              <h3>gggg</h3>
            </div>
            <hr>
            <div class="d-flex justify-content-between">
              <h3>Guild Messages:</h3>
              <h3>gggg</h3>
            </div>
            <hr>
            <div class="d-flex justify-content-between">
              <h3>Guild Emojis:</h3>
              <h3>gggg</h3>
            </div>
            <hr>
            <div class="d-flex justify-content-between">
              <h3>Guild Stickers:</h3>
              <h3>gggg</h3>
            </div>
            <hr>
            <div class="d-flex justify-content-between">
              <h3>Guild Bans:</h3>
              <h3>gggg</h3>
            </div>
            <hr>
            <div class="d-flex justify-content-between">
              <h3>Guild Creation Date:</h3>
              <h3>gggg</h3>
            </div>
          </div>
        </div>
        <div class="statify">
          <div class="section-header">
            <h1>[statify] Information</h1>
          </div>
          <div class="items">
            <div class="d-flex justify-content-between">
              <h3>Commands Used:</h3>
              <h3>gggg</h3>
            </div>
            <hr>
            <div class="d-flex justify-content-between">
              <h3>Last Used Command:</h3>
              <h3>gggg</h3>
            </div>
            <hr>
            <div class="d-flex justify-content-between">
              <h3>Most Used Command:</h3>
              <h3>gggg</h3>
            </div>
            <hr>
            <div class="d-flex justify-content-between">
              <h3>statify Join Date:</h3>
              <h3>gggg</h3>
            </div>
            <hr>
            <div class="d-flex justify-content-between">
              <h3>Version:</h3>
              <h3>gggg</h3>
            </div>
          </div>
        </div>
      </div>
    </div>
  </section>

  <%- include('./Blocks/footer') %>

    <script src="/js/statify.js"></script>
    <script src="/js/sidebar.js"></script>

