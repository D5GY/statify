              <img class="card-img-top" src="/images/Games/splitgate.png" alt="Card image cap">
              <div class="card-body">
                <div class="card-body">
                  usage: /splitgate username: LyK1Nw0Lf platform: xbox
                </div>
              </div>
            </div>
          </div>
        </div>
      </div>
      <div class="col-lg-12"></div>
    </div>
  </section>

    <%- include('./Blocks/footer') %>

    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/js/bootstrap.bundle.min.js"></script>
    <script src="https://cdn.jsdelivr.net/npm/popper.js@1.12.9/dist/umd/popper.min.js"></script>
    <script src="/js/statify.js"></script>
</body>

</html>